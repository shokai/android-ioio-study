<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
    <ToggleButton android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="ToggleButton" android:id="@+id/toggleButtonLED"></ToggleButton>
    <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/linearLayout3" android:weightSum="1">
        <TextView android:layout_width="72dp" android:id="@+id/textView3" android:text="Digital In" android:layout_height="wrap_content"></TextView>
        <SeekBar android:max="1" android:layout_height="wrap_content" android:progress="0" android:layout_width="wrap_content" android:layout_weight="0.13" android:id="@+id/seekBarDigitalIn"></SeekBar>
    </LinearLayout>
    <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/linearLayout1">
        <TextView android:id="@+id/textView1" android:text="PWM" android:layout_height="wrap_content" android:textAppearance="?android:attr/textAppearanceSmall" android:layout_width="48dp"></TextView>
        <SeekBar android:layout_weight="1" android:max="1000" android:layout_width="match_parent" android:progress="500" android:id="@+id/seekBarPwm" android:layout_height="wrap_content"></SeekBar>
    </LinearLayout>
    <LinearLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:id="@+id/linearLayout2">
        <TextView android:id="@+id/textView2" android:text="Analog In" android:layout_height="wrap_content" android:textAppearance="?android:attr/textAppearanceSmall" android:layout_width="74dp"></TextView>
        <SeekBar android:layout_weight="1" android:max="1000" android:layout_width="match_parent" android:progress="500" android:id="@+id/SeekBarAnalogIn" android:layout_height="wrap_content"></SeekBar>
    </LinearLayout>
</LinearLayout>
